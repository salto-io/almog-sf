salesforce.ListView Knowledge__kav_published_articles {
  fullName = "Knowledge__kav.published_articles"
  columns = [
    "TITLE",
    "ABSTRACT",
    "ARTICLE_NUMBER",
    "LANGUAGE",
  ]
  filterScope = "Everything"
  filters = [
    {
      field = "PUBLISH_STATUS"
      operation = "equals"
      value = "2"
    },
    {
      field = "LANGUAGE"
      operation = "equals"
      value = "en_US"
    },
  ]
  label = "Published Articles"
  _parent = [
    salesforce.Knowledge__kav,
  ]
  _alias = "Published Articles"
}
